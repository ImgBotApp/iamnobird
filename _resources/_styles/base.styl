html {
  min-height: 100%;
}
* {
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  -webkit-font-smoothing: antialiased;
  font-smoothing: antialiased;
  box-sizing: border-box;
}
body {
  height: 100%;
  overflow-y: auto;
  overflow-x: hidden;
  margin: 0;
  display: grid;
  grid-auto-columns: 5%;
  grid-auto-rows: 1rem;
  justify-items: start;
  align-items: start;
  grid-column-gap: 1rem;
}
.hidden {
  display: none;
  opacity: 0;
}
img {
  max-width: 100%;
  height: auto;
}
button {
  background: transparent;
  border: 1px solid #ff3431;
  border-radius: 75px;
  color: #ff3431;
  height: 4rem;
  font-size: 1rem;
  letter-spacing: .1em;
  padding: 1rem 3rem;
  text-transform: uppercase;
  cursor: pointer;
  font-family: typeface-secondary;
}
button:hover {
  background: #ff3431
  color: #ffe3e2;
  -webkit-transition: background-color 200ms ease-out;
  -moz-transition: background-color 200ms ease-out;
  -o-transition: background-color 200ms ease-out;
  transition: background-color 200ms ease-out;
}
.op50 {
  opacity: .5;
}
.op30 {
  opacity: .3;
}
.AlignRight {
  text-align: right;
}
.AlignCenter {
  margin: 0 auto;
}

.LeftBlockHalfWidth {
  grid-column: 3 / 9;
}
.RightBlockHalfWidth {
  grid-column: 11 / 20;
}
.LeftBlockTwoThirds {
  grid-column: 2 / 14;
}
.RightBlockOneThird {
  grid-column: 14 / 18;
  width: 80%;
}
.LeftBlockOneThird {
  grid-column: 2 / 6;
  width: 80%
}
.RightBlockTwoThirds {
